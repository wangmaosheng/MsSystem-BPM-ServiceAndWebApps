[Unit]
Description=MsSystem Identity

[Service]
WorkingDirectory=/webroot/mssystem/api/identity/linux-x64/publish
ExecStart=/usr/bin/dotnet /webroot/mssystem/api/identity/linux-x64/publish/MsSystem.Identity.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=mssystem-identity-log
User=root
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
[Install]
WantedBy=multi-user.target
