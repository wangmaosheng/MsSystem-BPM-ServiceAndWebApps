[Unit]
Description=MsSystem Web

[Service]
WorkingDirectory=/webroot/mssystem/web/publish
ExecStart=/usr/bin/dotnet /webroot/mssystem/web/publish/MsSystem.Web.dll
Restart=always

RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=MsSystem-Web
User=root
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target