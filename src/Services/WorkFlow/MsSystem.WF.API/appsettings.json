{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Audience": "mssystem_wf_api",
    "Authority": "http://gateway:5000"
  },
  "MySQL": {
    "Connection": "Database=mssystemwf;Data Source=mysql;User Id=root;Password=123456"
  },
  "RabbitMQ": {
    "HostName": "rabbitmq",
    "UserName": "admin",
    "Password": "admin"
  },
  "WorkFlow": {
    "Users": "/api/Sys/User/GetUserTreeAsync",
    "Roles": "/api/Sys/Role/GetRoleTreesAsync",
    "GetUserIds": "/api/Sys/User/GetUserIdsByRoleIdsAsync",
    "GetFlowNodeInfo": "/api/{0}/WorkFlow/GetFlowNodeInfo", //SQL获取节点信息
    "GetFinalNodeId": "/api/{0}/WorkFlow/GetFinalNodeId" //SQL获取最终的节点ID
  },
  "MsApplication": {
    "client_id": "mssystem",
    "client_secret": "123",
    "grant_type": "client_credentials",
    "scopes": "mssystem_api",
    "url": "http://gateway:5000",
    "tokenurl": "/connect/token"
  },
  "ZipkinOptions": {
    "ApplicationName": "wfapi",
    "ZipkinCollectorUrl": "http://zipkin:9411"
  },
  "ServiceDiscovery": {
    "Service": {
      "Name": "wfapi",
      "Address": "wfapi",
      "Port": 5003,
      "Interval": 10
    },
    "Consul": {
      "HttpEndpoint": "http://consul-agent-3:8500"
    }
  }
}
