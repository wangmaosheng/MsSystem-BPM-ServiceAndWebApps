[Unit]
Description=MsSystem OA

[Service]
WorkingDirectory=/webroot/mssystem/api/oa/linux-x64/publish
ExecStart=/usr/bin/dotnet /webroot/mssystem/api/oa/linux-x64/publish/MsSystem.OA.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=mssystem-oa-log
User=root
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
[Install]
WantedBy=multi-user.target
