[Unit]
Description=MsSystem Weixin

[Service]
WorkingDirectory=/webroot/mssystem/api/weixin/linux-x64/publish
ExecStart=/usr/bin/dotnet /webroot/mssystem/api/weixin/linux-x64/publish/MsSystem.Weixin.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=mssystem-weixin-log
User=root
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
[Install]
WantedBy=multi-user.target
